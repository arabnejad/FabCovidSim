
cd $job_results
$run_prefix

/usr/bin/env > env.log

start_time="$$(date -u +%s.%N)"

python3 $covid_dummy_exec --outputdir $output_dir --threads $cores United_Kingdom 2>&1 | tee output_log.txt

end_time="$$(date -u +%s.%N)"
elapsed="$$(bc <<<"$$end_time-$$start_time")"
echo "Total Executing Time = $$elapsed seconds" | tee -a "elapsed_time.txt"

